@inherits SvgBase

<SvgBase  Scale="Scale">
    <path id="@Id" style="stroke:@Color;stroke-width:@Width" d="M @SvgUtility.FloatToStringWithDot(Length + X),@SvgUtility.FloatToStringWithDot(Y) H @SvgUtility.FloatToStringWithDot(X)" />
</SvgBase>


@code { 
    [Parameter] public string Color { get; set; } = "black";
    [Parameter] public float Width { get; set; } = 1;
    [Parameter] public float Length { get; set; } = 1; 
}
